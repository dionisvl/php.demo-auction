{
    "name": "deworkerpro/demo-auction",
    "description": "",
    "type": "project",
    "license": "BSD-3-Clause",
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "php": "^8.0",
        "ext-mbstring": "^8.0",
        "doctrine/migrations": "^3.0",
        "doctrine/orm": "^2.7",
        "laminas/laminas-config-aggregator": "^1.2",
        "league/oauth2-server": "^8.2",
        "middlewares/negotiation": "^2.0",
        "monolog/monolog": "^2.0",
        "php-di/php-di": "^6.0",
        "ramsey/uuid": "^4.0",
        "sentry/sdk": "^3.1",
        "slim/psr7": "^1.0",
        "slim/slim": "^4.3",
        "symfony/cache": "^6.0",
        "symfony/config": "^6.0",
        "symfony/console": "^5.0",
        "symfony/mailer": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/validator": "^6.0",
        "twig/twig": "^3.0",
        "webmozart/assert": "^1.6"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.3",
        "dms/phpunit-arraysubset-asserts": "^0.3.0",
        "doctrine/data-fixtures": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "lctrs/psalm-psr-container-plugin": "^1.1",
        "overtrue/phplint": "^4.0",
        "phpunit/phpunit": "^9.2",
        "psalm/plugin-phpunit": "^0.16.0",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.3"
    },
    "replace": {
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "autoload": {
        "files": [
            "src/env.php"
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phplint",
        "php-cs-fixer": "php-cs-fixer --config=.php-cs-fixer.php --allow-risky=yes",
        "psalm": "psalm --config=psalm.xml",
        "test": "phpunit --colors=always",
        "test-coverage": "XDEBUG_MODE=coverage phpunit --colors=always --coverage-html var/test/coverage",
        "app": "php bin/app.php --ansi"
    }
}
